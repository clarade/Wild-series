{% extends "base.html.twig" %}
{% block title %}Saison no: #{{ season.number }}{% endblock %}

{% block body %}

<div class="media">
  <div class="media-body">
    <h1 class="mt-0">{{ program.title }}</h1>
    <p>{{ program.summary }}</p>
    <p>{{season.year}}
    <p>Synopsis : {{ season.description }}</p>
    {% for episode in season.episodes %}
    <p>Episode {{episode.number}} : {{episode.title}} </p>
    {% endfor %}
  </div>
</div>

{%endblock %}